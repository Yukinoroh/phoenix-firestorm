			<text name="ban_resident_label">
				Sempre prohibits:
			</text>
			<name_list name="banned_avatar_name_list">
				<columns label="Nom" name="name"/>
				<columns label="Data de l'última connexió" name="last_login_date"/>
				<columns label="Data de prohibició" name="ban_date"/>
				<columns label="Prohibit per" name="bannedby"/>
