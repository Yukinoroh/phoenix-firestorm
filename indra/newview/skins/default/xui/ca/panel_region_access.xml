		<panel label="GESTORS DEL DOMINI" name="estate_managers_panel">
			<text name="estate_manager_label">
				Gestors del domini:
			</text>
			<name_list name="estate_manager_name_list">
				<columns label="Nom" name="name"/>
			</name_list>
			<button label="Afegeix..." name="add_estate_manager_btn"/>
			<button label="Suprimeix..." name="remove_estate_manager_btn"/>
			<button label="Exporta..." name="export_estate_manager_btn"/>
		</panel>
		<panel label="PERMESOS" name="allowed_panel">
			<panel label="top_panel" name="allowed_search_panel">
				<filter_editor label="Cerca agents permesos" name="allowed_search_input"/>
				<button label="Copia" name="copy_allowed_list_btn"/>
			</panel>
			<text name="allow_resident_label">
				Sempre permesos:
			</text>
			<name_list name="allowed_avatar_name_list">
