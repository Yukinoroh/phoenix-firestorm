			<text name="allow_group_label">
				Grups sempre permesos:
			</text>
			<name_list name="allowed_group_name_list">
				<columns label="Nom" name="name"/>
			</name_list>
			<button label="Afegeix..." name="add_allowed_group_btn"/>
			<button label="Suprimeix" name="remove_allowed_group_btn"/>
			<button label="Exporta..." name="export_allowed_group_btn"/>
		</panel>
		<panel label="PROHIBITS" name="banned_panel">
			<panel label="top_panel" name="banned_search_panel">
				<filter_editor label="Cerca agents prohibits" name="banned_search_input"/>
