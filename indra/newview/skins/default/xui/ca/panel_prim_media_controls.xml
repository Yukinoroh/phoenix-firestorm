<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel name="MediaControls">
	<layout_stack name="progress_indicator_area">
		<layout_panel name="media_progress_indicator">
			<progress_bar name="media_progress_bar" tool_tip="Carregant mèdia"/>
		</layout_panel>
	</layout_stack>
	<layout_stack name="media_controls">
		<layout_panel name="back">
			<button name="back_btn" tool_tip="Anterior"/>
		</layout_panel>
		<layout_panel name="fwd">
			<button name="fwd_btn" tool_tip="Següent"/>
		</layout_panel>
		<layout_panel name="home">
			<button name="home_btn" tool_tip="Pàgina d'inici"/>
		</layout_panel>
		<layout_panel name="media_stop">
			<button name="media_stop_btn" tool_tip="Atura"/>
		</layout_panel>
		<layout_panel name="reload">
			<button name="reload_btn" tool_tip="Torna a carregar"/>
		</layout_panel>
		<layout_panel name="stop">
			<button name="stop_btn" tool_tip="Atura la càrrega"/>
		</layout_panel>
		<layout_panel name="play">
			<button name="play_btn" tool_tip="Reprodueix el mèdia"/>
		</layout_panel>
		<layout_panel name="pause">
			<button name="pause_btn" tool_tip="Pausa el mèdia"/>
		</layout_panel>
		<layout_panel name="media_address">
			<line_editor name="media_address_url" tool_tip="Adreça"/>
			<icon name="media_secure_lock_flag" tool_tip="Navegació segura"/>
			<layout_stack name="media_address_url_icons">
				<layout_panel name="media_address_url_icons_wl">
					<icon name="media_whitelist_flag" tool_tip="Llista blanca activada"/>
				</layout_panel>
			</layout_stack>
		</layout_panel>
		<layout_panel name="media_play_position">
			<slider_bar name="media_play_slider" tool_tip="Progrés de la reproducció del vídeo"/>
		</layout_panel>
		<layout_panel name="skip_back">
			<button name="skip_back_btn" tool_tip="Retrocedeix"/>
		</layout_panel>
		<layout_panel name="skip_forward">
			<button name="skip_forward_btn" tool_tip="Avança"/>
		</layout_panel>
		<layout_panel name="media_volume">
			<button name="media_mute_button" tool_tip="Silencia"/>
			<slider name="volume_slider" tool_tip="Volum"/>
		</layout_panel>
		<layout_panel name="zoom_frame">
			<button name="zoom_frame_btn" tool_tip="Fes zoom"/>
		</layout_panel>
		<layout_panel name="close">
			<button name="close_btn" tool_tip="Allunya el zoom"/>
		</layout_panel>
		<layout_panel name="new_window">
			<button name="new_window_btn" tool_tip="Obre l'URL al navegador"/>
		</layout_panel>
	</layout_stack>
</panel>
