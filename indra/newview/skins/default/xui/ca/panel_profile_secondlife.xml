			<menu_button name="image_action_btn" tool_tip="Realitza accions relacionades amb la foto del perfil" />
		</layout_panel>
		<layout_panel name="label_stack">
			<text name="label" value="Data de naixement:"/>
			<text_editor name="user_age" value="(carregant...)"/>
			<text name="label2" value="Compte:"/>
