				<layout_panel name="badge_layout">
					<layout_stack name="badge_stacker">
						<layout_panel name="top_badge_layout">
							<icon name="top_badge_icon" tool_tip="Empleat(da) de Linden Lab" />
						</layout_panel>
						<layout_panel name="bottom_badge_layout">
							<icon name="bottom_badge_icon" tool_tip="Empleat(da) de Linden Lab" />
						</layout_panel>
					</layout_stack>
				</layout_panel>
			</layout_stack>
			<text name="partner_label" value="Parella:"/>
		</layout_panel>
	</layout_stack>
